{% extends 'TutoresBundle::layout.html.twig' %}


{% block content %}
    {% set version = constant('Symfony\\Component\\HttpKernel\\Kernel::MAJOR_VERSION') ~ '.' ~ constant('Symfony\\Component\\HttpKernel\\Kernel::MINOR_VERSION')%}

    <h1 class="title">Bienvenido!</h1>

    <p>Tutores App</p>
    {% if result %}
        Registros procesados : {{ result.getTotalProcessedCount() }} <br />
        Registros correctos : {{ result.getSuccessCount() }} <br />
        Registros fallidos : {{ result.getErrorCount() }} <br />
    {%  for exception in result.getExceptions() %}
        <li>{{ exception.getMessage() }}</li>
    {% endfor %}
    {% endif %}

    {{ form_start(form) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}
{% endblock %}

